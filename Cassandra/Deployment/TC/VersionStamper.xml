<Project DefaultTargets="SetVersion" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <Import Project="Common.xml" />

    <Target Name="SetVersion">
        <Error Condition="'$(SolutionDir)' == ''" Text="SolutionDir is not defined" />
        <Error Condition="'$(Revision)' == ''" Text="Revision is not defined" />
        <Message Text='Revision=$(Revision)' />

        <Exec Command='"$(SolutionDir)..\..\Assemblies\VersionStamper.exe" $(SolutionDir) $(Revision)' />
        <!--<Message Text='"$(SolutionDir)..\Assemblies\VersionStamper.exe" $(SolutionDir)Front done!' />-->
    </Target>
</Project>