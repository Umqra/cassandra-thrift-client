<?xml version="1.0" encoding="utf-8"?>
<log4net>
  <appender name="CommandExecuterAppender" type="log4net.Appender.FileAppender">
    <file value="logs\TestLog.txt" />
    <appendToFile value="true" />
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%date %-6timestamp %-5level %logger %property %message%newline" />
    </layout>
  </appender>
  <root>
    <level value="DEBUG" />
    <appender-ref ref="CommandExecuterAppender" />
  </root>
</log4net>
